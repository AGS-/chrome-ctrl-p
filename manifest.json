{
  "manifest_version": 2,

  "name": "chrome-ctrl-p",
  "description": "This extension searches through your open tabs and matches strings with the content",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "MacCtrl+Shift+P"
      }
    }
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}
